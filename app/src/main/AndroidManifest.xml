<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.machine.calculatorbmiapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CalculatorBMIApp">
        <activity
            android:name=".name_surname"
            android:exported="false" />
        <activity
            android:name=".bfi_woman_table"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".bfi_man_table"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".bmr_table"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".bmi_table"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>